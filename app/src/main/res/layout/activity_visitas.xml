<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/phitecnicos2"
    android:scaleType="centerCrop"
    android:visibility="visible">

        <Button
            android:id="@+id/insertar"
            android:layout_width="172dp"
            android:layout_height="120dp"
            android:layout_marginTop="136dp"
            android:layout_marginEnd="56dp"
            android:background="@drawable/boton_redondo"
            android:text="Insertar Visita"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="86dp"
            android:layout_height="88dp"
            android:layout_marginTop="160dp"
            android:visibility="visible"
            app:layout_constraintEnd_toStartOf="@+id/insertar"
            app:layout_constraintHorizontal_bias="0.742"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/insertar" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="161dp"
            android:layout_marginTop="76dp"
            android:layout_marginEnd="195dp"
            android:text="Visitas"
            android:textSize="40sp"
            android:textStyle="bold"
            android:textColor="#5E5A5A"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.26"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/eliminar"
            android:layout_width="172dp"
            android:layout_height="120dp"
            android:layout_marginTop="264dp"
            android:layout_marginEnd="56dp"
            android:background="@drawable/boton_redondo"
            android:text="Eliminar Visita"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="86dp"
            android:layout_height="88dp"
            android:layout_marginStart="37dp"
            android:layout_marginTop="280dp"
            android:layout_marginEnd="33dp"
            android:visibility="visible"
            app:layout_constraintEnd_toStartOf="@+id/eliminar"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/eliminar" />

        <Button
            android:id="@+id/actualizar"
            android:layout_width="172dp"
            android:layout_height="120dp"
            android:layout_marginTop="396dp"
            android:layout_marginEnd="56dp"
            android:background="@drawable/boton_redondo"
            android:text="Actualizar Visita"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="86dp"
            android:layout_height="88dp"
            android:layout_marginStart="37dp"
            android:layout_marginTop="412dp"
            android:layout_marginEnd="33dp"
            android:visibility="visible"
            app:layout_constraintEnd_toStartOf="@+id/eliminar"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/actualizar" />

        <Button
            android:id="@+id/mostrar"
            android:layout_width="172dp"
            android:layout_height="120dp"
            android:layout_marginTop="528dp"
            android:layout_marginEnd="56dp"
            android:background="@drawable/boton_redondo"
            android:text="Mostrar Visitas"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="86dp"
            android:layout_height="88dp"
            android:layout_marginStart="37dp"
            android:layout_marginTop="540dp"
            android:layout_marginEnd="33dp"
            android:visibility="visible"
            app:layout_constraintEnd_toStartOf="@+id/eliminar"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/mostrar" />

        <Button
            android:id="@+id/menu"
            android:layout_width="81dp"
            android:layout_height="55dp"
            android:layout_marginBottom="12dp"
            android:text="Menu"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.048"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/personal"
            android:layout_width="122dp"
            android:layout_height="55dp"
            android:layout_marginBottom="12dp"
            android:text="Personal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.927"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/linea"
            android:layout_width="493dp"
            android:layout_height="52dp"
            android:layout_marginStart="133dp"
            android:layout_marginEnd="220dp"
            android:layout_marginBottom="64dp"
            android:text="_____________________________________________________________________________"
            android:textColor="#000000"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.4"
            app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
