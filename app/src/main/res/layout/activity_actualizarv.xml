<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/phitecnicos2"
    android:scaleType="centerCrop">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="195dp"
        android:text="Introduce un ID de Visita a Actualizar"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.441"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bActualizar"
        android:layout_width="144dp"
        android:layout_height="79dp"
        android:layout_marginStart="158dp"
        android:layout_marginTop="616dp"
        android:layout_marginEnd="159dp"
        android:background="@drawable/boton_redondo"
        android:text="ACTUALIZAR"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.54"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="195dp"
        android:text="Visitas"
        android:textSize="40sp"
        android:textStyle="bold"
        android:textColor="#5E5A5A"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Spinner
        android:id="@+id/sID"
        android:layout_width="149dp"
        android:layout_height="45dp"
        android:layout_marginTop="188dp"
        android:background="@drawable/spinner"
        android:backgroundTint="#BDBDBD"
        android:backgroundTintMode="add"
        android:breakStrategy="simple"
        android:drawableTint="#FFFFFF"
        android:ems="10"
        android:foregroundTint="#674C4C"
        android:hint="Id"
        android:inputType="text"
        android:paddingLeft="20dp"
        android:singleLine="true"
        android:textColor="@color/white"
        android:textColorHint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/trabajadoresexternos"
        android:layout_width="354dp"
        android:layout_height="65dp"
        android:layout_marginTop="432dp"
        android:background="@drawable/contorno"
        android:backgroundTint="#BDBDBD"
        android:backgroundTintMode="add"
        android:breakStrategy="simple"
        android:drawableTint="#000000"
        android:ems="10"
        android:foregroundTint="#674C4C"
        android:hint="Trabajadores Externos"
        android:inputType="text"
        android:paddingLeft="20dp"
        android:singleLine="true"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.456"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/nombreempresa"
        android:layout_width="220dp"
        android:layout_height="41dp"
        android:layout_marginTop="268dp"
        android:background="@drawable/contorno"
        android:backgroundTint="#BDBDBD"
        android:backgroundTintMode="add"
        android:breakStrategy="simple"
        android:drawableTint="#000000"
        android:ems="10"
        android:foregroundTint="#674C4C"
        android:hint="Nombre de la Empresa"
        android:inputType="text"
        android:paddingLeft="20dp"
        android:singleLine="true"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.136"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/strabajadores"
        android:layout_width="220dp"
        android:layout_height="35dp"
        android:layout_marginTop="376dp"
        android:background="@drawable/spinner"
        android:backgroundTint="#BDBDBD"
        android:backgroundTintMode="add"
        android:breakStrategy="simple"
        android:drawableTint="#FFFFFF"
        android:ems="10"
        android:foregroundTint="#674C4C"
        android:paddingLeft="5dp"
        android:singleLine="true"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.136"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/fecha"
        android:layout_width="130dp"
        android:layout_height="41dp"
        android:layout_marginTop="268dp"
        android:background="@drawable/contorno"
        android:backgroundTint="#BDBDBD"
        android:backgroundTintMode="add"
        android:breakStrategy="simple"
        android:drawableTint="#000000"
        android:ems="10"
        android:foregroundTint="#674C4C"
        android:hint="Fecha"
        android:inputType="date"
        android:paddingLeft="20dp"
        android:singleLine="true"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.985"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/stvisita"
        android:layout_width="230dp"
        android:layout_height="34dp"
        android:layout_marginTop="320dp"
        android:background="@drawable/spinner"
        android:backgroundTint="#BDBDBD"
        android:backgroundTintMode="add"
        android:breakStrategy="simple"
        android:drawableTint="#FFFFFF"
        android:ems="10"
        android:foregroundTint="#674C4C"
        android:paddingLeft="5dp"
        android:singleLine="true"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.276"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/observaciones"
        android:layout_width="347dp"
        android:layout_height="84dp"
        android:layout_marginTop="520dp"
        android:background="@drawable/contorno"
        android:backgroundTint="#BDBDBD"
        android:backgroundTintMode="add"
        android:breakStrategy="simple"
        android:drawableTint="#000000"
        android:ems="10"
        android:foregroundTint="#674C4C"
        android:hint="Observaciones"
        android:inputType="text"
        android:paddingLeft="20dp"
        android:singleLine="true"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.406"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>